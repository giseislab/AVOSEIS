#!/bin/csh -f


set segdb = "dbseg/quakes"
set archivedb = "db/archive"

# pause orb2dbt writes
orb2db_msg $segdb pause
orb2db_msg $archivedb pause
sleep 15

rtdbclean_mitch -m mitch@giseis.alaska.edu -p pf/rtdbclean_mitch.pf humpy dbseg/quakesdbevents

orb2db_msg $segdb pause
dbcrunch $segdb

# continue orb2dbt writes
orb2db_msg $segdb continue 
orb2db_msg $archivedb continue 
